transform pos_left:
    xpos  480
transform pos_right:
    xpos 1440
transform pos_center:
    xpos 960
transform bounce:
    ease 0.2 yoffset 20
    ease 0.2 yoffset -20
transform pop:
    zoom 0.5
    ease 0.4 zoom 1.0
transform slide_right:
    ease 0.2 xoffset 480
transform slide_left:
    ease 0.2 xoffset -480
transform slump:
    ease 0.2 yoffset -70
transform unslump:
    ease 0.2 yoffset 20
transform creeped_right:
    ease 0.2 xoffset 100
transform creeped_left:
    ease 0.2 xoffset -100

#show = Show something
#bounce = A vertical movement
#pop = A sudden appearance
#slide = A horizontal movement
#slump = lower the character in y axis
#creeped = slide the character slowly to the side